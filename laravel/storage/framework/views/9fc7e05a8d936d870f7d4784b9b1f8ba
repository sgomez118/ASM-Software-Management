<?php $__env->startSection('users'); ?>
<table class="table table-striped">
<?php $count = 1; ?>
		<thead>
			<th>#</th>
			<td>Name</td>
			<td>Email</td>
			<td>Type</td>
		</thead>
		<tbody>
			<?php foreach(App\User::all() as $user): ?>
				<tr data-userID="<?php echo e($user->id); ?>">
					<td><?php echo e($count++); ?></td>
					<td><?php echo e($user->first_name.' '.$user->last_name); ?></td>
					<td><?php echo e($user->email); ?></td>
					<td><?php echo e($user->type); ?></td>
				</tr>
			<?php endforeach; ?>
		</tbody>
</table>
<?php echo $__env->yieldSection(); ?>